{
	"compilerOptions": {
		"jsx": "react-jsx",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"typeRootes": ["./node_modules/@types", "./app/types/*.d.ts"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"paths": {
			"@/*": ["./app/*"]
		}
	},
	"exclude": ["node_modules"],
	"include": ["**/*.ts", "**/*.tsx"]
}
